{
  "name": "WeatherExtension",
  "displayName": "Get the weather",
  "description": "Get temperature and cloud cover on the bottom left of your status bar",
  "version": "0.1.3",
  "publisher": "AndrewMoll",
  "engines": {
    "vscode": "^0.10.1"
  },
  "license": "MIT",
  "homepage": "https://github.com/AndrewMoll/VSCode-Weather-Extension/README.md",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Weather",
    "Rain",
    "Fun",
    "Sunny"
  ],
  "icon": "Decorative_Sun.png",
  "galleryBanner":{
    "color":"",
    "theme":"light"
  },
  "bugs": {
    "url": "https://github.com/AndrewMoll/VSCode-Weather-Extension/issues",
    "email": ""
  },
  "activationEvents": [
    "onCommand:extension.Weather"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.Weather",
        "title": "Get Weather"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "open": "0.0.5"
  }
}
